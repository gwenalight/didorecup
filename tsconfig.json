{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "app/*": ["src/app/*"],
      "boot/*": ["src/boot/*"],
      "security/*": ["src/security/*"]
    },
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "importHelpers": true
  },
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "useWebpackText": true
  },
  "compileOnSave": false,
  "buildOnSave": false
}
