  "scripts": {
    "build:dev": "npm run prebuild && webpack --config config/webpack.dev.js",
    "build:prod": "npm run prebuild && mkdirp dist/artifacts && webpack --config config/webpack.prod.js --progress --profile",
    "build:package": "npm run build:prod",
    "ci": "npm run test:reports && npm run lint:ci && npm run e2e",
    "clean:dist": "rimraf -- dist .stats",
    "clean:all": "npm cache clean && npm run rimraf -- node_modules doc coverage dist aot ngfactory reports .stats .sonarlint",
    "docs": "npm run typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e --elementExplorer",
    "e2e": "concurrently --kill-others --success first \"npm run server:prod\" protractor",
    "lint": "mkdirp reports/tslint && npm run tslint \"src/**/*.ts\"",
    "lint:ci": "mkdirp reports/tslint && npm run tslint:ci \"src/**/*.ts\"",
    "ng": "ng",
    "prebuild": "npm run clean:dist",
    "pree2e": "npm run prebuild && mkdirp dist/artifacts && npm run webdriver:update && webpack --config config/webpack.test.e2e.js --progress --profile",
    "prettier:format": "prettier --write \"{e2e/**/,src/**/}*.{md,json,ts,css,scss,yml}\"",
    "protractor": "protractor -- --webdriver-update false",
    "server:dev": "webpack-dev-server --mode development --watch-content-base --config config/webpack.dev.js",
    "server:prod": "node config/express.js",
    "start": "npm run server:dev",
    "test": "rimraf reports && mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "test:watch": "rimraf reports && mocha-webpack --opts config/mocha/mocha-webpack.opts --watch",
    "test:coverage": "rimraf reports && cross-env NODE_ENV=coverage nyc --reporter=text mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "test:reports": "rimraf reports && cross-env NODE_ENV=coverage nyc mocha-webpack --opts config/mocha/mocha-webpack.opts --reporter=mocha-multi-reporters --reporter-options configFile=config/mocha/mocha-reports-config.json",
    "test:server": "webpack-dev-server --mode development --config config/webpack.test.browser.js",
    "tslint": "tslint --format stylish --project tsconfig.json -c tslint.json --force",
    "tslint:ci": "tslint --format json --project tsconfig.json -c tslint.json -o reports/tslint/tslint.json --force",
    "typedoc": "typedoc",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update -- --gecko false --standalone false --versions.chrome 2.41",
    "webpack-dev-server": "webpack-dev-server"
  },
